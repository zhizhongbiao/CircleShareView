<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--圆型控件-->
    <declare-styleable name="CircleView">
        <attr name="circle_color" format="color" />
    </declare-styleable>

    <!--画线控件-->
    <declare-styleable name="PercentLineView">
        <attr name="line_color" format="color" />
        <attr name="line_height" format="dimension" />
    </declare-styleable>

    <!--曲线图控件-->
    <declare-styleable name="BrokenLineStatisticsTableView">
        <attr name="broken_line_color" format="color" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
        <attr name="circle_point_color" format="color" />
    </declare-styleable>


</resources>